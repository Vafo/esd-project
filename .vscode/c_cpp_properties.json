{
    "env": {
        "PROJ_DIR": "${workspaceFolder}",
        "PROJ_STUPID_INCL_PATH" : "${env:PROJ_DIR}/**"
    },
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${env:PROJ_STUPID_INCL_PATH}"
            ],
            "defines": [
                "__AVR_ATmega128__"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "intelliSenseMode": "linux-gcc-x86",
            "compilerPath": "/usr/bin/avr-gcc",
            "cStandard": "c99",
            "cppStandard": "gnu++14"
        }
    ],
    "version": 4
}